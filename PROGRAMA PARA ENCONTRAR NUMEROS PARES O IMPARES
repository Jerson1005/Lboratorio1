#include <stdio.h>
int main() {
int num;
// Solicitar al usuario que ingrese un número
printf("Ingrese un número entero: ");
scanf("%d", &num);
// Determinar si el número es par o impar
if (num % 2 == 0) {
printf("El número %d es par.\n", num);
} else {
printf("El número %d es impar.\n", num);
}
return 0;
}
